{
  "name": "blog-rappitenderos-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --configuration=development,es-mx",
    "start-campaign-mx": "ng serve --open --project='mxCampaign'",
    "build": "ng build",
    "build-develop": "npm run rappi:prerender:es-mx",
    "build-camapigns-mx": "ng build --prod --project='mxCampaign'",
    "build-co": "npm run rappi:prerender:es-co",
    "build-br": "npm run rappi:prerender:pt-br",
    "build-mx": "npm run rappi:prerender:es-mx",
    "build-ar": "npm run rappi:prerender:es-ar",
    "build-cl": "npm run rappi:prerender:es-cl",
    "build-pe": "npm run rappi:prerender:es-pe",
    "build-ec": "npm run rappi:prerender:es-ec",
    "build-cr": "npm run rappi:prerender:es-cr",
    "build-uy": "npm run rappi:prerender:es-uy",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "dev:ssr": "ng run blog-rappitenderos-frontend:serve-ssr",
    "serve:ssr": "node dist/blog-rappitenderos-frontend/server/main.js",
    "build:ssr": "ng build && ng run blog-rappitenderos-frontend:server",
    "prerender": "ng run blog-rappitenderos-frontend:prerender",
    "dev:prerender": "ng run blog-rappitenderos-frontend:prerender --configuration=development",
    "prestage:prerender": "node prerender-utils.js stage en",
    "stage:prerender": "ng run blog-rappitenderos-frontend:prerender --configuration=stage --routes-file routes.txt",
    "prestage:prerender:es-co": "node prerender-utils.js stage en",
    "stage:prerender:es-co": "ng run blog-rappitenderos-frontend:prerender --configuration=stage,es-co --routes-file routes.txt",
    "prerappi:prerender:es-co": "node prerender-utils.js production es-CO",
    "rappi:prerender:es-co": "ng run blog-rappitenderos-frontend:prerender --configuration=rappi,es-co --routes-file routes.txt",
    "prerappi:prerender:pt-br": "node prerender-utils.js production pt-BR",
    "rappi:prerender:pt-br": "ng run blog-rappitenderos-frontend:prerender --configuration=rappi,pt-br --routes-file routes.txt",
    "prerappi:prerender:es-mx": "node prerender-utils.js production es-MX",
    "rappi:prerender:es-mx": "ng run blog-rappitenderos-frontend:prerender --configuration=rappi,es-mx --routes-file routes.txt",
    "prerappi:prerender:es-ar": "node prerender-utils.js production es-AR",
    "rappi:prerender:es-ar": "ng run blog-rappitenderos-frontend:prerender --configuration=rappi,es-ar --routes-file routes.txt",
    "prerappi:prerender:es-cl": "node prerender-utils.js production es-CL",
    "rappi:prerender:es-cl": "ng run blog-rappitenderos-frontend:prerender --configuration=rappi,es-cl --routes-file routes.txt",
    "prerappi:prerender:es-pe": "node prerender-utils.js production es-PE",
    "rappi:prerender:es-pe": "ng run blog-rappitenderos-frontend:prerender --configuration=rappi,es-pe --routes-file routes.txt",
    "prerappi:prerender:es-ec": "node prerender-utils.js production es-EC",
    "rappi:prerender:es-ec": "ng run blog-rappitenderos-frontend:prerender --configuration=rappi,es-ec --routes-file routes.txt",
    "prerappi:prerender:es-cr": "node prerender-utils.js production es-CR",
    "rappi:prerender:es-cr": "ng run blog-rappitenderos-frontend:prerender --configuration=rappi,es-cr --routes-file routes.txt",
    "prerappi:prerender:es-uy": "node prerender-utils.js production es-UY",
    "rappi:prerender:es-uy": "ng run blog-rappitenderos-frontend:prerender --configuration=rappi,es-uy --routes-file routes.txt",
    "lint": "ng lint",
    "start-local": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "12.2.13",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/platform-server": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@nguniversal/express-engine": "^12.1.0",
    "angular-code-input": "^1.6.0",
    "angular-google-tag-manager": "1.4.4",
    "animate.css": "^4.1.1",
    "countries-and-timezones": "^3.3.0",
    "express": "^4.15.2",
    "jwt-decode": "^3.1.2",
    "ng-recaptcha": "^8.0.1",
    "ngx-bootstrap": "^7.1.0",
    "ngx-owl-carousel-o": "^6.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.4",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.2.0",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.32.0",
    "eslint-plugin-folders": "^1.0.3",
    "eslint-plugin-unicorn": "^39.0.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "qs": "^6.10.3",
    "typescript": "~4.3.5"
  }
}
